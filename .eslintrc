{
	"root": true,
	"extends": [
		"eslint-config-niksy",
		"eslint-config-niksy/next",
		"eslint-config-prettier"
	],
	"plugins": ["eslint-plugin-prettier", "eslint-plugin-unicorn"],
	"globals": {
		"globalThis": false
	},
	"rules": {
		"prettier/prettier": 1,
		"camelcase": 0,
		"unicorn/prefer-flat-map": 0
	},
	"overrides": [
		{
			"files": "mock/*.js",
			"env": {
				"browser": true
			}
		}
	]
}
